{
  "name": "@laura-wert/vue-permission-management",
  "version": "0.1.1",
  "repository": {
    "url": "github.com/LauraWert/vue-permission-management",
    "type": "git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "",
  "license": "MIT",
  "private": false,
  "main": "dist/",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "yarn lint && rimraf ./dist && webpack --config ./webpack.config.js",
